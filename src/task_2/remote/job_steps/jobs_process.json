[
  {
    "Name": "Service CSV to Parquet",
    "ActionOnFailure": "CONTINUE",
    "HadoopJarStep": {
      "Jar": "command-runner.jar",
      "Args": [
        "spark-submit",
        "--deploy-mode",
        "cluster",
        "--master",
        "yarn",
        "--conf",
        "spark.yarn.submit.waitAppCompletion=true",
        "s3a://{work_bucket}/process/service_to_parquet.py.py",
        "--exec-date",
        "{exec_date}",
        "--bronze-bucket",
        "{bronze_bucket}",
        "--silver-bucket",
        "{silver_bucket}"
      ]
    }
  },
  {
    "Name": "Traffic CSV to Parquet",
    "ActionOnFailure": "CONTINUE",
    "HadoopJarStep": {
      "Jar": "command-runner.jar",
      "Args": [
        "spark-submit",
        "--deploy-mode",
        "cluster",
        "--master",
        "yarn",
        "--conf",
        "spark.yarn.submit.waitAppCompletion=true",
        "s3a://{work_bucket}/process/traffic_to_parquet.py.py",
        "--exec-date",
        "{exec_date}",
        "--bronze-bucket",
        "{bronze_bucket}",
        "--silver-bucket",
        "{silver_bucket}"
      ]
    }
  }
]